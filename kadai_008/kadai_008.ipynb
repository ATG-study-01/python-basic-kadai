      "source": [
        "var = 30\n",
        "\n",
        "#変数varが3の倍数と5の倍数の両方に該当する場合はFizzBuzzを出力\n",
        "if var % 15 == 0 :\n",
        "    print(\"FizzBuzz\")\n",
        "#変数varが3の倍数の場合はFizzを出力\n",
        "elif var % 3 == 0:\n",
        "    print(\"Fizz\")\n",
        "#変数varが5の倍数の場合はBuzzを出力\n",
        "elif var % 5 == 0:\n",
        "    print(\"Buzz\")\n",
        "#上記どれにも該当しない場合はvarの値を出力\n",
        "else:\n",
        "    print(var)"
